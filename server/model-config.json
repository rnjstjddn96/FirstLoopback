{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "accessToken": {
    "dataSource": "mongoDs",
    "public": false
  },
  "ACL": {
    "dataSource": "mongoDs",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDs",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongoDs",
    "public": false
  },
  "user": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "emailVerificationRequired": true,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "login": true,
          "createUserByParams": true,
          "me": true,
          "updateUser": true
        }
      }
    }
  },
  "address": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "createAddressByUser": true,
          "findAll": true,
          "me": true,
          "updateById": true,
          "deleteById": true,
          "deleteAddress": true
        }
      }
    }
  }
}
